config globals 'globals'
	option packet_steering '1'

config device
	option name 'br-lan'
	option type 'bridge'
	list ports 'eth0'

config interface 'lan'
	option proto 'static'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option ipaddr '192.168.5.2'
	option gateway '192.168.5.1'
	option delegate '0'
	option device 'eth0'
	list dns '192.168.5.1'
